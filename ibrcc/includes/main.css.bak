img {
	padding: 0px;
	margin: 0px;
	border: 0px;
	vertical-align: bottom;
}

a {
	color: #000;
}

a.redlink {
	color: #972400;
	text-decoration: none;
}
a.redlink:hover {
	text-decoration: underline;
}

/* form stuff */

label {
	color: #1b4e81;
	font-family: tahoma;
	font-weight: bold;
	font-size: 14px;
}

td {
	padding: 2px 5px 2px 5px;
}
td.formtitle {
	text-align: right;
}

/* sexy box */
