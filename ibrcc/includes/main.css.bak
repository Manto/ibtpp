body {
	margin: 0px;
	padding: 0px;
	font-family: tahoma, helvetica;
	font-size: 13px;
}

div {
}

div.main {
	margin-left: auto;
	margin-right: auto;
	width: 780px;
	padding: 10px 0px 15px 0px;
}

div.column {
	float: left;
	padding: 0px 0px 15px 0px;
}

div.header {
	font-family: arial narrow, helvetica;
	letter-spacing: -0.3px;
	color: #1b4e81;
	font-size: 21px;
	font-weight: bold;
}

div.boxheader {
	font-family: tahoma;
	color: #fff;
	font-size: 14px;
	font-weight: bold;
}

div.footer {
	width: 100%;
	background-color: #333;
	color: #fff;
	text-align: center;
	padding: 10px;
	font-size: 14px;
	clear:both;
}

div.box1 {
	width: 239px;
	height: 72px;
	margin: 8px;
	padding: 10px;
	color: #fff;
	background-image: url('../images/box1bg.png');
	background-repeat: none;
}

